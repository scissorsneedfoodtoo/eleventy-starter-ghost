{
  "name": "eleventy-starter-ghost",
  "version": "1.0.0",
  "description": "A starter template to build websites with Ghost & Eleventy",
  "main": ".eleventy.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env ELEVENTY_ENV=dev eleventy --serve",
    "build": "cross-env ELEVENTY_ENV=prod eleventy",
    "test": "npm run build",
    "lint": "eslint . --ext .js --cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TryGhost/eleventy-starter-ghost.git"
  },
  "keywords": [
    "eleventy",
    "ghost",
    "starter",
    "boilerplate",
    "static",
    "site",
    "jamstack"
  ],
  "author": "David Darnes",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TryGhost/eleventy-starter-ghost/issues"
  },
  "homepage": "https://github.com/TryGhost/eleventy-starter-ghost#readme",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-plugin-rss": "^1.1.1",
    "@tryghost/content-api": "^1.5.4",
    "@tryghost/helpers": "^1.1.43",
    "clean-css": "^5.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "eslint-utils": "^2.1.0",
    "html-minifier": "^4.0.0",
    "i18next": "^20.2.2",
    "i18next-fs-backend": "^1.1.1",
    "terser": "^5.7.0"
  },
  "devDependencies": {
    "@11ty/eleventy-img": "^0.8.3",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "eslint": "^7.25.0",
    "eslint-plugin-ghost": "^2.1.0",
    "probe-image-size": "^7.2.1"
  }
}
